#------------------------------------------armstrong
'''for num in range(1, 10000+1):
    n = len(str(num))
    sum = 0
    x = num
    while x > 0:
        dig = x % 10
        sum += dig ** n
        x = x // 10
    if sum == num :
       print(num, end='  ')'''
#-----------------------------------------------sezar
'''word = input('Enter str: ')
for letter in word:
    asc = ord(letter)  #convert each character to ascii code by ord function
    if asc != 120 and asc != 121 and asc != 122:
        alp = chr(asc + 3)
    elif asc == 120:
        alp = chr(97)
    elif asc == 121:
        alp = chr(98)
    elif asc == 122:
        alp = chr(99)
    print(alp, end='')'''
#----------------------------------------newton-raphson
'''def f(x):
    return x**3 - x**2 + 2


def df(x):
    return 3*x**2 - 2*x


for x0 in range(1, 10+1):
    newraph = x0 - (f(x0))/(df(x0))
    print("newton_raphson", {x0}, "=", newraph)'''
